
  <view class="container">
    <view class="userinfo" bindtap="handleViewTap">
      <image class="userinfo-avatar" src="{{ userInfo.avatarUrl }}" background-size="cover"/>
      <view class="userinfo-nickname">{{ userInfo.nickName }}</view>
    </view>

    <button class="tel-btn plain" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" disabled>电话授权(暂无功能)</button>
    <button class="company-btn selected" bindtap="goToCompanyInfo">已绑定的公司</button>
  </view>